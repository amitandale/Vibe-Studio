# Supabase Lane Environment Template
# Secrets are injected at runtime from ops/supabase/lanes/credentials.env.
# Identity
COMPOSE_PROJECT_NAME=supa-{{LANE}}
LANE={{LANE}}
VOL_NS={{VOLUME_NAMESPACE}}

# Postgres connection
PGHOST=127.0.0.1
PGHOST_PORT={{PGHOST_PORT}}
PGPORT=5432
PGDATABASE={{PGDATABASE}}
PGUSER=postgres
SUPABASE_SUPER_ROLE={{SUPABASE_SUPER_ROLE}}

# Service ports and paths
KONG_HTTP_PORT={{KONG_HTTP_PORT}}
EDGE_PORT={{EDGE_PORT}}
EDGE_ENV_FILE={{EDGE_ENV_FILE}}

# Secrets
JWT_SECRET={{JWT_SECRET}}
ANON_KEY={{ANON_KEY}}
SERVICE_ROLE_KEY={{SERVICE_ROLE_KEY}}
