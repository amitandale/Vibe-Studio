# Supabase Lane Environment Template
# DO NOT COMMIT REAL SECRETS. Populate the runner-local copy in $SUPABASE_STATE_DIR/lanes/<lane>.env.
# Identity
COMPOSE_PROJECT_NAME=supa-{{LANE}}
LANE={{LANE}}
VOL_NS={{VOLUME_NAMESPACE}}

# Postgres connection
PGHOST=127.0.0.1
PGPORT={{PGPORT}}
PGDATABASE={{PGDATABASE}}
PGUSER=postgres
PGPASSWORD={{PGPASSWORD}}
SUPABASE_SUPER_ROLE={{SUPABASE_SUPER_ROLE}}
# Independent credential for the maintenance superuser (e.g. supabase_admin)
SUPABASE_SUPER_PASSWORD={{SUPABASE_SUPER_PASSWORD}}

# Service ports and paths
KONG_HTTP_PORT={{KONG_HTTP_PORT}}
EDGE_PORT={{EDGE_PORT}}
EDGE_ENV_FILE={{EDGE_ENV_FILE}}

# Secrets
JWT_SECRET={{JWT_SECRET}}
ANON_KEY={{ANON_KEY}}
SERVICE_ROLE_KEY={{SERVICE_ROLE_KEY}}
